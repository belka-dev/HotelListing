{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Overide": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.LifeTime": "Information"
      }
    },
    
      "WriteTo": [
        {
          "Name": "File",
          "Args": {
            "path": "./logs/log-txt",
            "rollingInterval": "Day"
          }
        },
        {
          "Name": "Seq",
          "Args": { "ServerUrl":  "http://localhost:5341" }
        }
      ]
  },
  "AllowedHosts": "*"
}
